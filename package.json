{
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "netlify-lambda": "^1.2.0",
    "prettier": "^1.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
